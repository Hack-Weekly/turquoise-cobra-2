rules_version = '2';
service cloud.firestore {
  match /databases/{database}/documents {
  	match /messages/{message} {
      allow read: if true;
      allow create: if request.auth != null;
      allow delete: if request.auth.uid == resource.data.author.uid;
    }
  	match /channels/{channel} {
      allow read: if true;
    }
  }
}